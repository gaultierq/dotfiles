#!/bin/bash
#Generated by JetBrains Toolbox 2.7.0.48109 at 2025-07-28T12:26:29

declare -a intellij_args=()
declare -- wait=""

for o in "$@"; do
  if [[ "$o" = "--wait" || "$o" = "-w" ]]; then
    wait="-W"
    o="--wait"
  fi
  if [[ "$o" =~ " " ]]; then
    intellij_args+=("\"$o\"")
  else
    intellij_args+=("$o")
  fi
done

open -na "/Applications/RubyMine 2.app/Contents/MacOS/rubymine" $wait --args "${intellij_args[@]}"