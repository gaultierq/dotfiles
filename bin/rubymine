#!/bin/bash
#Generated by JetBrains Toolbox 3.0.1.59888 at 2025-11-09T15:32:40

declare -a intellij_args=()
declare -- wait=""

for o in "$@"; do
  if [[ "$o" = "--wait" || "$o" = "-w" ]]; then
    wait="-W"
    o="--wait"
  fi
  if [[ "$o" =~ " " ]]; then
    intellij_args+=("\"$o\"")
  else
    intellij_args+=("$o")
  fi
done

open -na "/Applications/RubyMine.app/Contents/MacOS/rubymine" $wait --args "${intellij_args[@]}"