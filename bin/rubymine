#!/bin/bash
#Generated by JetBrains Toolbox 2.5.4.38621 at 2025-03-23T09:29:36

declare -a intellij_args=()
declare -- wait=""

for o in "$@"; do
  if [[ "$o" = "--wait" || "$o" = "-w" ]]; then
    wait="-W"
    o="--wait"
  fi
  if [[ "$o" =~ " " ]]; then
    intellij_args+=("\"$o\"")
  else
    intellij_args+=("$o")
  fi
done

open -na "/Applications/RubyMine.app/Contents/MacOS/rubymine" $wait --args "${intellij_args[@]}"