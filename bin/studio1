#!/bin/bash
#Generated by JetBrains Toolbox 2.7.0.48109 at 2025-07-25T15:05:59

declare -a intellij_args=()
declare -- wait=""

for o in "$@"; do
  if [[ "$o" = "--wait" || "$o" = "-w" ]]; then
    wait="-W"
    o="--wait"
  fi
  if [[ "$o" =~ " " ]]; then
    intellij_args+=("\"$o\"")
  else
    intellij_args+=("$o")
  fi
done

open -na "/Applications/Android Studio.app/Contents/MacOS/studio" $wait --args "${intellij_args[@]}"