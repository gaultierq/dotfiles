#!/bin/bash
#Generated by JetBrains Toolbox 2.4.2.32922 at 2024-09-14T16:24:29.799461

declare -a intellij_args=()
declare -- wait=""

for o in "$@"; do
  if [[ "$o" = "--wait" || "$o" = "-w" ]]; then
    wait="-W"
    o="--wait"
  fi
  if [[ "$o" =~ " " ]]; then
    intellij_args+=("\"$o\"")
  else
    intellij_args+=("$o")
  fi
done

open -na "/Applications/Android Studio.app/Contents/MacOS/studio" $wait --args "${intellij_args[@]}"